<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent" android:layout_height="match_parent">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver"
        android:id="@+id/rodape_to_Supermercado"
        android:layout_alignTop="@+id/rodape"
        android:layout_alignStart="@+id/voltarMapa" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voltar "
        android:id="@+id/voltarMapa"
        android:layout_marginEnd="11dp"
        android:layout_alignBottom="@+id/rodape"
        android:layout_alignParentEnd="true"
        android:visibility="invisible"/>


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:id="@+id/rodape"
        android:src="@drawable/side_nav_bar"
        android:maxHeight="90dp"
        android:maxWidth="60dp"
        android:layout_gravity="center_horizontal|top"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:text="Supermercado 1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/supermercadoSelecionadoWaypoint"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="20sp"
        android:textColor="@color/cast_expanded_controller_text_color"
        android:layout_marginStart="37dp"
        android:layout_alignBottom="@+id/rodape_to_Supermercado"
        android:layout_alignParentStart="true" />

</RelativeLayout>