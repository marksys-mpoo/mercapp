<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_lista_produtos"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.mercapp.supermercado.gui.ListaProdutos">

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/lista_produtos"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView6" />

    <Button
        android:text="Voltar Adm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="changeTelaListaProdutosToTelaAdmimistrador"
        android:id="@+id/button10"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="IdSuper"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:layout_above="@+id/lista_produtos"
        android:layout_alignParentEnd="true"
        android:id="@+id/textView7" />

    <TextView
        android:text="Id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:id="@+id/textView6"
        android:layout_below="@+id/button10"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Descrição"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:id="@+id/textView4"
        android:layout_above="@+id/lista_produtos"
        android:layout_toStartOf="@+id/button10" />

    <TextView
        android:text="Preço"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:layout_below="@+id/button10"
        android:layout_toStartOf="@+id/textView7"
        android:layout_marginEnd="29dp"
        android:id="@+id/textView8" />

</RelativeLayout>
